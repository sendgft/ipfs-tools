{
  "name": "@sendgft/ipfs-tools",
  "version": "1.0.0",
  "description": "IPFS tools for sendGFT",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "bin": "bin/ipfs-tools",
  "files": [
    "dist",
    "bin",
    "data",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "watch": "tsc --watch",
    "ipfs-tools": "bin/ipfs-tools",
    "release": "yarn build && semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sendgft/ipfs-tools.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "ipfs",
    "sendgft",
    "gft"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sendgft/ipfs-tools/issues"
  },
  "homepage": "https://github.com/sendgft/ipfs-tools#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/glob": "^7.2.0",
    "husky": "^8.0.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@pinata/sdk": "^1.1.23",
    "chalk": "4",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "execa": "4",
    "glob": "^7.2.0",
    "got": "11",
    "ipfs-http-client": "52",
    "ipfs-utils": "8.1.6"
  },
  "resolutions": {
    "ipfs-http-client/**/ipfs-utils": "8.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
